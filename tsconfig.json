{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    // "baseUrl": ".",
    "paths": {
      "@api/*": ["../api/src/*"],
      "@root/*": ["./src/*"],
      "@ui/*": ["./src/ui/*"],
    },
    "types": [
      "./worker-configuration.d.ts",
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}